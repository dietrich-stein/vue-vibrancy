{
  "name": "vue-vibrancy",
  "version": "1.0.0",
  "private": false,
  "main": "dist/vibrancy.umd.js",
  "module": "dist/vibrancy.esm.js",
  "unpkg": "dist/vibrancy.umd.min.js",
  "homepage": "https://github.com/dietrich-stein/vue-vibrancy",
  "author": {
    "name": "Dietrich Stein",
    "url": "https://github.com/dietrich-stein"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "vue",
    "vibrancy",
    "background",
    "effect"
  ],
  "files": [
    "src/*",
    "dist/*",
    "package.json",
    "README.md",
    "LICENSE"
